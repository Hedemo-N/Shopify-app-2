{
  "version": 2,
  "builds": [{ "src": "index.ts", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.ts" },
    { "src": "/auth(.*)", "dest": "index.ts" },
    { "src": "/onboarding.html", "dest": "/public/onboarding.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "index.ts" }
  ]
}
